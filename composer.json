{
    "name": "alexgoncharcherkassy/mailchimpbundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Simple bundle for Mailchimp",
    "keywords": ["symfony", "mailchimp", "email", "mailing"],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/framework-bundle": "^3.4|^4.0",
        "sensio/framework-extra-bundle": "^3.4|^4.0|^5.0",
        "symfony/yaml": "^3.4|^4.0",
        "symfony/serializer-pack": "*",
        "symfony/orm-pack": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "symfony/phpunit-bridge": "*",
        "symfony/browser-kit": "^3.0|^4.0"
    },
    "authors": [
        {
            "name": "Alex Gonchar",
            "email": "cs210785gaa@gmail.com"
        }
    ],
    "autoload": {
      "psr-4": { "AlexCk\\MailchimpBundle\\": "src/AlexCk/MailchimpBundle" }
    },
    "autoload-dev": {
      "psr-4": { "AlexCk\\MailchimpBundle\\": "tests/AlexCk/MailchimpBundle" },
      "classmap": ["tests/TestKernel.php"]
    }
}
