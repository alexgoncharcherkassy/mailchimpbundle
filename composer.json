{
    "name": "alexgoncharcherkassy/mailchimpbundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Simple bundle for Mailchimp",
    "keywords": ["symfony", "mailchimp", "email", "mailing"],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "symfony/framework-bundle": "^3.3|^4.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "symfony/monolog-bundle": "^3.0",
        "fzaninotto/faker": "~1.4",
        "sensio/framework-extra-bundle": "^3.0|^4.0|^5.0",
        "friendsofphp/php-cs-fixer": "^2.1",
        "symfony/browser-kit": "^3.0|^4.0",
        "symfony/validator": "^3.0|^4.0",
        "symfony/expression-language": "^3.0|^4.0",
        "symfony/serializer": "^3.0|^4.0",
        "symfony/property-info": "^3.0|^4.0",
        "symfony/yaml": "^3.0|^4.0",
        "symfony/phpunit-bridge": "*"
    },
    "authors": [
        {
            "name": "Alex Gonchar",
            "email": "cs210785gaa@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
      "psr-4": { "AlexCk\\MailchimpBundle\\": "src/AlexCk/MailchimpBundle" }
    },
    "autoload-dev": {
      "psr-4": { "AlexCk\\MailchimpBundle\\": "tests/AlexCk/MailchimpBundle" },
      "classmap": ["tests/TestKernel.php"]
    }
}
